class Magic
{
  public static void main(String args[])
  {
    int num=109, store, sum=0;
    while(num>9)
    {
      while(num>0)
      {
        store=num%10;
        num=num/10;
        sum=sum+store;
      }
      num=sum;
      sum=0;
      if(num==1)
      System.out.println("It is a magic number");
      else
      System.out.println("It is not a magic number");
   }
 }
}
