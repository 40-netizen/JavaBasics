class Special
{
  public static void main(String args[])
  {
    int num=20, factorial, temporary=num, sum=0, store, i=0;
    while(temporary!=0)
    {
      store=temporary%10;
      factorial=1;
      while(i<=store)
      {
        factorial=factorial*store;
        i+=1;
      }
      sum=sum+factorial;
      temporary=temporary/10;
    }
    if(sum==num)
    System.out.println("It is a special number");
    else
    System.out.println("It is not a special number");
 }
}
