class Armstrong
{
  public static void main(String args[])
  {
    int n=231, temp=n, s=0, a;
    while(n>0)
    {
      a=n%10;
      s=s+(a*a*a);
      n=n/10;
    }
    if(temp==s)
    {
      System.out.println("It is an armstrong number");
    }
    else
    {
      System.out.println("It is not an armstrong number");
    }
 }
}
